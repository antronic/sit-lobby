{
  "name": "kim-web",
  "version": "0.0.1-alpha",
  "description": "Kontent Individual Management Web Interface",
  "main": "index.js",
  "repository": "https://github.com/antronic/KIM-WEB",
  "author": "Jirachai Chansivanon <antronic.inc@gmail.com>",
  "license": "private",
  "private": true,
  "scripts": {
    "dev": "node scripts/dev"
  },
  "dependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-emotion": "^8.0.12",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "4.0.0-beta",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "emotion": "^8.0.12",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "graceful-fs": "^4.1.11",
    "html-webpack-plugin": "^2.30.1",
    "ora": "^1.3.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.0.0",
    "react-emotion": "^8.0.12",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.25.1",
    "redux": "^3.7.2",
    "shelljs": "^0.7.8",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "zepto": "^1.2.0"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.7.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.7.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9"
  },
  "babel": {
    "presets": [
      "react"
    ],
    "plugins": [
      "emotion"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "alias.js"
        }
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "global-require": 0,
      "class-methods-use-this": 0,
      "no-param-reassign": 0,
      "no-unused-vars": 1,
      "react/jsx-uses-vars": [
        "error"
      ],
      "semi": 2,
      "jsx-a11y/href-no-hash": 0,
      "react/forbid-prop-types": 0,
      "react/self-closing-comp": 0,
      "react/no-unknown-property": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "eslintIgnore": [
    "config",
    "scripts",
    "node_modules"
  ]
}
